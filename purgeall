@bot.command()
@commands.has_permissions(manage_guild=True)
async def purgeall(ctx):
    await ctx.send('# До запуска 5 минут.\nДля отмены нажать ^C в терминале.')
    time.sleep(60)
    await ctx.send('# До запуска 4 минуы.\nДля отмены нажать ^C в терминале.')
    time.sleep(60)
    await ctx.send('# До запуска 3 минуты. \nДля отмены нажать ^C в терминале.')
    time.sleep(60)
    await ctx.send('# До запуска 2 минуты. \nДля отмены нажать ^C в терминале.')
    time.sleep(60)
    await ctx.send('# До запуска 1 минута. \nТы знал, что делаешь.')
    time.sleep(60)
    await ctx.send('Приступаю.')

    for member in ctx.guild.members:
        if member.bot:
            continue
        if member.id == ctx.guild.owner.id:
            continue
        await member.kick(reason='Округ Редвью закрылся. Приносим свои извинения.')
        await ctx.send(f'{member.name} Был изгнан.')
